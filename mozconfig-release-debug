# add common options here, such as making an optimized release build
mk_add_options MOZ_PARALLEL_BUILD=4

#Same as the release build except that there should hopefully be something debugging wise in there
ac_add_options --enable-optimize --enable-debug

# Build Firefox
mk_add_options MOZ_OBJDIR=@TOPSRCDIR@/obj-ff
ac_add_options --enable-project=browser
